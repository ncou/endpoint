{
  "name": "phapi/endpoint",
  "description": "Contains endpoint class that all Phapi Endpoints should extend. Has built in functionality for handling OPTIONS requests.",
  "keywords": [
    "endpoint", "phapi"
  ],
  "version": "1.0.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://phapi.github.io",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "phapi/contract": "1.*",
    "phapi/exception": "1.*",
    "psr/http-message": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phapi\\": "src/Phapi/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phapi\\Tests\\": "tests/Phapi/"
    }
  }
}